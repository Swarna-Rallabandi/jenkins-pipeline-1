pipeline {
    agent {
        label 'java-slave4'
    }
    tools {
        maven 'MAVEN_3.8.8' // the same name should be configured in the tools section
    }
    stages {
        stage ('maven'){
            steps {
                echo " this is maven sectoin"
                sh 'mvn --version'
            }
        }
        stage ('secondstage'){
            tools {
                maven 'MAVEN_3.9.6'
            }
            steps {
                echo "hell for msecomnd stage"
                sh 'mvn --version'
            }
        }
            
    }
}
